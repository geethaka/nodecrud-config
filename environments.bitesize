project: btdemo
environments:
  - name: dev
    namespace: btdemo-dev
    deployment:
      method: rolling-upgrade
    services:
      - name: nodecrud
        external_url: btdemo.pearson.com
        ssl: true
        port: 80
      - name: mysqldb
        port: 3306
        replicas: 1
        env:
          - name: MYSQL_ROOT_PASSWORD
            value: password		  
          - name: MYSQL_DATABASE
            value: nodejs